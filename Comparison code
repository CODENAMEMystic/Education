import UIKit

class ViewController: UIViewController {
    
    
    
    @IBOutlet weak var teamname: UITextField!
    @IBOutlet weak var email: UITextField!
    @IBOutlet weak var username: UITextField!
    func getSheet(url: String) {
        let url = URL(string: url)
        
        let task = URLSession.shared.dataTask(with: url!) { data, response, error in
            guard error == nil else {
                print(error)
                return
            }
            
            guard let data = data else {
                print("Data is empty")
                return
            }
            
            let json =
                try! JSONSerialization.jsonObject(with: data, options: []) as! Dictionary<String, AnyObject>
            
            let people = json["feed"]!["entry"] as! NSArray
            let person1 = people[0] as! Dictionary<String, AnyObject>
            let user = person1["gsx$username"]!["$t"]!
            let email = person1["gsx$email"]!["$t"]!
            let team = person1["gsx$teamname"]!["$t"]!
            
            // instantly update the UI
            DispatchQueue.main.async(execute: {
                self.username.text = user  as? String
                self.email.text = email as? String
                self.teamname.text = team as? String
            })
        }
        task.resume()
    }

    override func viewDidLoad() {
        super.viewDidLoad()
        let sheet = "https://spreadsheets.google.com/feeds/list/1e_P5UsfO8hc48pE4Ltk22eu29m3-dn6IsIArB9V_Az0/od6/public/values?alt=json"
        getSheet(url: sheet);
    }

    override func didReceiveMemoryWarning() {
        super.didReceiveMemoryWarning()
        // Dispose of any resources that can be recreated.
    }


}
